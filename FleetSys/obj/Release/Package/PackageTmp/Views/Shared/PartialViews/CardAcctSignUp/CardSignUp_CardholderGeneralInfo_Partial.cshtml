@using FleetOps.Helpers;
@using ModelSector.Global_Resources;
@model  ModelSector.CardHolderInfoModel

<div class="panel panel-default">

    <div class="panel-heading">
        <div class="panel-actions pull-left">
            <a href="javascript:;" ng-click="modalClick();" class="btn-minimize"><i class="fa fa-plus"></i></a>
            <a href="javascript:;" class="btn-close"><i class="fa fa-refresh"></i></a>
        </div>
    </div>

    <div class="panel-body">
        <div id="cardHolder" class="zerotop">
            <table id="tblCards" class="table bootstrap-datatable datatable small-font" data-dtable options="dtOptions">
                <thead>
                    <tr>
                        <th style="width: 200px">Applicant ID</th>
                        <th>Card Type</th>
                        <th>Emboss Name</th>
                        <th>Driver Name</th>
                        <th>VRN</th>
                        <th>Card No</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
