@using FleetOps.Helpers;
@using ModelSector.Global_Resources;
@model ModelSector.GeneralInfoModel



<style type="text/css">
    .sub-nav-bar {
        border-bottom: 1px solid #eee;
        padding-bottom: 10px;
    }
</style>
<h1>Account Listing</h1>
<br />
<style type="text/css">
    .panel .panel-heading .panel-actions a i.fa {
        color: #666666;
        font-weight: bold;
    }
</style>

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="panel-heading">
            <a href="javascript:;" ng-click="refresh();" class="btn-close"><i class="fa fa-refresh"></i></a>
        </div>
    </div>

    <div class="panel-body">
        <div class="table-holder">
            <table class="table bootstrap-datatable datatable small-font" id="tblAcctCorp" data-dtable options="dtOptions">
                <thead>
                    <tr>
                        <th>Acct No.</th>
                        <th>Company Name</th>
                        <th>Status</th>
                        <th>Plastic Type</th>
                        <th>Company Reg No.</th>
                        <th>Creation Date</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
