
@{
    ViewBag.Title = "Corporate Codes";
    ViewBag.ng_app = "corporateCodesApp";
    Layout = "~/Views/Shared/_ModuleLayout.cshtml";
}
@section ModuleSideBar
{
    <style type="text/css">
        .form-group {
            border-bottom: 1px dashed #eee;
        }

        .btn-group-justified .btn {
            color: #fff;
        }
    </style>
    <div class="sidebar-collapse" routing-menu>
        <div class="sidebar-header">
            <div class="btn-group btn-group-justified">
                <a href="#/" data-redirect="index" class="btn btn-primary btn-sm" role="button"><i class="fa fa-list"></i>&nbsp; Show List</a>
                <a href="#/new" data-redirect="new" class="btn btn-success btn-sm" role="button"><i class="fa fa-plus"></i>&nbsp; New</a>
            </div>
        </div>
        <div class="sidebar-menu">
            <ul class="nav nav-sidebar">
                <li><a ng-show="$root.obj._type==='edit'" href="#/details/{{$root.obj.corpCd}}"><span class="text">General Info</span></a></li>
                <li><a ng-show="$root.obj._type==='edit'" href="#/velocity/{{$root.obj.corpCd}}"><span class="text">Contacts</span></a></li>
                <li><a ng-show="$root.obj._type==='edit'" href="#/securityDeposit/{{$root.obj.corpCd}}"><span class="text">Address</span></a></li>
                <li><a ng-show="$root.obj._type==='edit'" href="#/contacts/{{$root.obj.corpCd}}"><span class="text">Txn Search</span></a></li>
                <li><a ng-show="$root.obj._type==='edit'" href="#/address/{{$root.obj.corpCd}}"><span class="text">Status Maintenance</span></a></li>
            </ul>
        </div>
    </div>
}



<div class="main" style="opacity:1; min-height: 658px;">
    <div class="row">
        <div class="col-lg-12">
            <ol class="breadcrumb">
                <li><a href="#">Home</a></li>
                <li><a href="#">Corporate Codes</a></li>
                <li class="active">Index</li>
                @*<li class="active">{{root.obj.corpcd}}</li>*@
            </ol>
        </div>
        <div class="col-lg-12">
            <ng-view></ng-view>
            <script type="text/ng-template" id="index.html">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h2><i class="fa fa-list"></i>Dealers List</h2>
                        </div>
                        <div class="panel-body">
                            <table dtable options="dtOptions" class="table bootstrap-datatable datatable small-font">
                                <thead>
                                    <tr>
                                        <th>Merchant Id</th>
                                        <th>Site Id</th>
                                        <th>Business Name</th>
                                        <th>DBA Name</th>
                                        <th>Payee Name</th>
                                        <th>CoReg No</th>
                                        <th>DBA State</th>
                                        <th>Person In Charge</th>
                                        <th>Current Status</th>
                                        <th>Creation Date</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </script>
            
        </div>
    </div>
</div>
@section CustomPageScript
{
    <script src="~/Scripts/angular-sanitize.js"></script>
    <script src="~/Scripts/uiselect.js"></script>
    <link href="~/Scripts/uiselect.css" rel="stylesheet" />
    <link href="~/Scripts/select2.css" rel="stylesheet" />
    <script src="~/ng/Utils.js"></script>
    <script src="~/ng/Apps/Corporate%20Codes/corporatecodes.js"></script>
    <link href="~/Content/css/wizard.css" rel="stylesheet" />
    <script src="~/Content/css/wizard.js"></script>
    <script src="~/Content/css/formWizard.js"></script>
}

